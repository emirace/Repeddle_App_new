{
  "expo": {
    "name": "Repeddle_App",
    "slug": "Repeddle_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emirace.repeddle-app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emirace.Repeddle_App"
    },
    "scheme": "repeddle-app",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-asset",
      "expo-secure-store",
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.359040935611-1kijfv8vvtn9ghvehrqqciv2dm7sme3m"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c576fd6e-ba91-4e0b-96d5-d0c3705759fd"
      }
    }
  }
}
